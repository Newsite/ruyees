<%@ page contentType="text/html;charset=UTF-8"%><%@ taglib prefix="j" uri="/WEB-INF/tlds/formtag.tld"%><%@ taglib prefix="tags" tagdir="/WEB-INF/tags"%><j:set name="ctx" value="${pageContext.request.contextPath}" /><!DOCTYPE html><html lang="zh_CN"><head><link rel="stylesheet" href="${ctx}/resources/css/bootstrap.min.css"><link rel="stylesheet" href="${ctx}/resources/css/bootstrap-responsive.min.css"><link rel="stylesheet" href="${ctx}/resources/css/otp_iframe.css"><link rel="stylesheet" href="${ctx}/resources/css/carousel.css"><script type="text/javascript" src="${ctx}/resources/js/lib/jquery/jquery.min.js"></script><script src="${ctx}/resources/js/lib/bootstrap/bootstrap.min.js" type="text/javascript"></script><script type="text/javascript" src="${ctx}/resources/js/lib/plugin/flash/jquery.jcarousel.min.js"></script><script type="text/javascript" src="${ctx}/resources/js/lib/plugin/flash/carousel.js"></script><script type="text/javascript" src="${ctx }/resources/js/lib/jquery-form/form.js"></script><script type="text/javascript" src="${ctx}/resources/js/lib/bootstrap/bootstrap.min.js"></script><script type="text/javascript" src="${ctx}/resources/js/iframe_inner.js"></script><j:if test="${empty index }">  <j:set name="index" value="0" /></j:if></head><body oncontextmenu="return false" onselectstart="return false" ondragstart="return false" onbeforecopy="return false" onmouseup="document.selection.empty()" oncopy="document.selection.empty()" onselect="document.selection.empty()">  <form class="form-horizontal" method="post" id="ajaxform" action="#" name="form">    <input type="hidden" name="fdId" value="${content.examCategories[index].fdId}" />    <div class="iframe-container">      <div class="stepsTab-cont-top">        <p class="location">          <a href="${ctx}/trainee/step/${name}/1/1"><b class="sjl"></b><b class="sjl2"></b>1.1 学习入门视频<b class="sjr"></b><b            class="sjr2"></b></a> <a class="active" href="${ctx}/trainee/step/${name}/1/2"><b class="sjl"></b><b            class="sjl2"></b>1.2 参加在线测试<b class="sjr"></b><b class="sjr2"></b></a>        </p>        <p>          <label class="label">闯关说明</label>&nbsp;${content.content.item.fdRemarke}        </p>      </div>      <div class="media toMe">        <a class="pull-left" href="#"> <tags:image href="${member.guid.poto}" clas="media-object img-polaroid" />        </a>        <div class="media-body">          <div class="icon-quote pull-left"></div>          <p>            ${content.content.fdRemarke }<i class="icon-quote"></i>          </p>        </div>      </div>      <j:ifelse test="${throughStatus[1][2] == 1}">        <j:then>          <div class="success_box well">            <div>              	恭喜你完成&nbsp;<span class="text-info">业务学习</span>&nbsp;关卡！请进入下一关&nbsp;               <span class="text-info"><a href="#" rel="tooltip" onclick="goTop(function(){window.parent.activeStep(2);});">学术准备</a></span>&nbsp;               <a href="#" rel="tooltip" title="点击进入" class="icon-nextStep" onclick="goTop(function(){window.parent.activeStep(2);});"></a>            </div>          </div>        </j:then>        <j:else>          <div class="success_box well hide">            <div>              恭喜你完成&nbsp;<span class="text-info">业务学习</span>&nbsp;关卡！请进入下一关&nbsp;<span class="text-info">学术准备</span>&nbsp;<a href="#"                rel="tooltip" title="点击进入" class="icon-nextStep"                onclick="goTop(function(){window.parent.activeStep(2);});"></a>            </div>          </div>        </j:else>      </j:ifelse>      <div class="row-container clearfix">        <div id="" class="pull-left content-box">          <p class="fs14">            正在测试&nbsp;<span class="text-info">${content.examCategories[index].fdName}</span>          </p>          <dl class="examList">            <dt></dt>            <dd></dd>            <j:iter items="${content.examCategories[index].bamExams}" var="exam" status="s">              <dt>${s.index + 1}.&nbsp;${exam.fdSubject}</dt>              <input type="hidden" name="examOptions[${s.index }].fdId" value="${exam.fdId}" />              <dd>                <ol>                  <j:iter items="${exam.options}" var="option" status="status">                    <li><input type="${exam.htmlTag}" name="examOptions[${s.index}].values"                      value="${status.index + 1}" />${option}</li>                  </j:iter>                  <!-- 这里应该有个提示信息 -->                </ol>              </dd>            </j:iter>          </dl>          <div align="center" id="submitDiv">            <j:if test="${content.examCategories[index].through==false }">              <button type="button" id="butsubmit" class="btn btn-info">提交</button>            </j:if>          </div>        </div>        <div id="carouselWrap" class="pull-right">          <ul id="carousel" class="unstyled">            <j:set name="operId" value="${content.examCategories[index].fdId}" />            <j:iter items="${content.examCategories }" var="cate" status="s">              <j:ifelse test="${operId == cate.fdId}">                <j:then>                  <li class="active"><a href="${ctx}/trainee/step/${name}/1/2/${cate.fdId }"> <img                      class="img-polaroid" src="${ctx}/resources/images/video-img.jpg" width="133" height="72"> <j:ifelse                        test="${cate.through }">                        <j:then>                          <span class="text-success">通过</span>                        </j:then>                      </j:ifelse>                  </a>                    <h5 style="overflow: hidden;" title="${cate.fdName}">${cate.fdName}</h5></li>                </j:then>                <j:else>                  <li><a href="${ctx}/trainee/step/${name}/1/2/${cate.fdId }"> <img class="img-polaroid"                      src="${ctx}/resources/images/video-img.jpg" width="133" height="72"> <j:ifelse                        test="${cate.through }">                        <j:then>                          <span class="text-success">通过</span>                        </j:then>                      </j:ifelse>                  </a>                    <h5 style="overflow: hidden;" title="${cate.fdName}">${cate.fdName}</h5></li>                </j:else>              </j:ifelse>            </j:iter>          </ul>          <j:ifelse test="${throughStatus[1][2] == 1}">            <j:then>              <button class="btn btn-block btn-info btn-next" type="button">                下一关<i class="icon-nextStep"></i>              </button>            </j:then>            <j:else>              <button class="btn btn-block btn-info btn-next" type="button" disabled>                下一关<i class="icon-nextStep"></i>              </button>            </j:else>          </j:ifelse>        </div>      </div>    </div>  </form></body><script type="text/javascript">	$("#butsubmit").click(function(e) {		var empty = false;		$(".examList dd ol").each(function(i) {			if ($(this).find(":radio:checked,:checkbox:checked").val() == undefined) {				empty = true;			}		});		if (empty) {			e.preventDefault();			window.parent.ifraAlert2("请在完成全部试题以后，点击提交按钮。");			return false;		} else {			var data = $("#ajaxform").serializeArray();			jQuery.post("${ctx}/ajax/exam/pushExam", data,			RateArticleSuccess, "json");		}	});	function RateArticleSuccess(data) {		if (data.throng) {// 全答对			$("#submitDiv").hide();			if (data.next == null) {// 通过本关				goTop(function() {					succBox('恭喜你完成&nbsp;<span class="text-info">业务学习</span>&nbsp;关卡！请进入下一关&nbsp;<span class="text-info"><a href="#" rel="tooltip" onclick="goTop(function(){window.parent.activeStep(2);});">学术准备</a></span>&nbsp;', '#', "点击进入");					$(".success_box .icon-nextStep").click(function() {						window.parent.activeStep(2);					});					if (data.phaseThrong == true) {						$(".btn-next").removeAttr("disabled").attr("onclick", "window.parent.activeStep(2)");						$(".success_box").removeClass("hide");					}				});			} else {// 通过本试卷				window.parent.ifraAlert3("恭喜你全答对了!", function() {					goTop(succBox('恭喜你完成<span class="text-info">${content.examCategories[index].fdName}</span>关卡！请进入下一试卷&nbsp;<span class="text-info"><a href="${ctx}/trainee/step/${name}/1/2/' + data.next.fdId + '">' + data.next.fdName + '</a></span>&nbsp;', '${ctx}/trainee/step/${name}/1/2/' + data.next.fdId + '', "点击进入下一试卷"));				});			}		} else {			window.parent.ifraAlert3("答对<b class='point'>" + data.right + "</b>题，答错<b class='point'>" + data.error + "</b>题！", function() {				goTop(succBox('很遗憾，闯关失败。你可以选择<span class="text-info"><a href="${ctx}/trainee/step/${name}/1/2">继续答题</a></span>或者<span class="text-info"><a href="${ctx}/trainee/step/${name}/1/1">再次学习视频</a></span>', '${ctx}/trainee/step/${name}/1/1', "点击返回"));			});		}	}	$(".btn-next").click(function() {		goTop(function() {			window.parent.activeStep(2);		});	});</script></html>