<%@ page contentType="text/html;charset=UTF-8"%><%@ taglib prefix="j" uri="/WEB-INF/tags/formtag.tld"%><%@ taglib prefix="tags" tagdir="/WEB-INF/tags"%><j:set name="ctx" value="${pageContext.request.contextPath}" /><!DOCTYPE html><html lang="zh_CN"><head><link href="${ctx}/resources/css/select-step.css" rel="stylesheet" type="text/css"><script type="text/javascript" src="${ctx}/resources/js/modernizr.js"></script><!--[if lte IE 8]><style>	.lock-bg .lock{	z-index:23;	opacity:1;	}	.ch-thumb:after{	background:transparent;	}  .support-note .note-ie {    display: block;  }</style><![endif]--></head><body>  <div class="container">    <div class="steps-banner">      <div class="media">        <a class="pull-left" href="#"><tags:image            href="${member.guid.poto}" clas="media-object img-polaroid" />        </a>        <div class="media-body">          <div class="speak">            ${member.newTeach.realName}老师，你好！我是你的指导教师&nbsp;<span class="text-info">${member.guid.realName}</span>&nbsp;。要完成新教师备课任务并最终成为合格、优秀的新东方教师，你需要冲破以下&nbsp;<span              class="text-info">6</span>&nbsp;道关卡。别担心，我会全程指导你的。现在点击第一关开始吧，加油！          </div>          <div align="right" class="blue">            	联系TA&nbsp;<a href="mailto:${member.guid.fdEmail}?subject=请多关照" rel="tooltip" title="发送邮件" class="icon-mail"></a>            		       <a href="${ctx}/notify/toAdd/${member.guid.fdId}" rel="tooltip" title="站内消息" class="icon-online"></a>          </div>        </div>      </div>    </div>    <div class="support-note">     <!--  <span class="note-ie alert alert-error fade in"> <a class="close" data-dismiss="alert" href="#">&times;</a>        您当前使用的是Microsoft Internet Explorer 8 或者更低版本的浏览器。为获得更佳用户体验，建议进行<a        href="http://windows.microsoft.com/zh-CN/internet-explorer/download-ie">升级</a>。      </span> -->    </div>    <section class="main">      <ul class="ch-grid">        <j:iter items="${statges}" var="stage" status="s">          <li>            <div class="ch-item">              ${stage.selectIsLock }              <div class="trans">                <j:ifelse test="${stage.enable}">                  <j:then>                    <a href="${ctx }/trainee/main/${name}/${s.index+1 }/${stage.goTo}" class="vMid-box">                      ${stage.templateItem.description} </a>                  </j:then>                  <j:else>                    <a class="vMid-box">${stage.templateItem.description}</a>                  </j:else>                </j:ifelse>              </div>              <div class="ch-thumb ch-img-${s.index+1}">                <j:ifelse test="${stage.enable}">                  <j:then>                    <a href="${ctx }/trainee/main/${name }/${s.index+1 }/${stage.goTo}" class="vMid-box">                      <div>                        第 <i class="icon-num${s.index+1}">${s.index+1}</i> 关                      </div> ${stage.templateItem.fdName }                    </a>                  </j:then>                  <j:else>                    <div class="vMid-box">                      <div>                        第 <i class="icon-num${s.index+1}">${s.index+1}</i> 关                      </div>                      ${stage.templateItem.fdName }                    </div>                  </j:else>                </j:ifelse>              </div>            </div>          </li>        </j:iter>      </ul>    </section>  </div>  <script type="text/javascript" src="${ctx }/resources/js/select-step.js"></script></body></html>