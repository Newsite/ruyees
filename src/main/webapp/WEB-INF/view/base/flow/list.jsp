<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="j" uri="/WEB-INF/tlds/formtag.tld"%><%@ taglib uri="http://www.springframework.org/tags" prefix="spring"%><j:set name="ctx" value="${pageContext.request.contextPath}" /><!DOCTYPE html><html lang="zh_CN"><head></head><body>  <div class="well">    <div class="page-header">      <a href="${ctx}/dashboard" class="tile-return">返回桌面</a>      <h4>组织备课</h4>      <p>在本模块中，您可以组织新教师与指导教师开展备课活动。</p>    </div>    <form class="" id="inputForm" action="" name="form">      <table class="table ">        <thead>          <tr>            <th width="10px">#</th>            <th width="32%">集团模板</th>            <th>备课活动</th>            <th width="10%">状态</th>            <th width="10%">组织者</th>            <!-- <th>发起日期</th>  -->            <th width="20%">操作</th>          </tr>        </thead>        <tbody>          <j:iter items="${bamProjects}" var="bean" status="vstatus">            <tr>              <td>${vstatus.index+1}</td>              <td>${bean.template.fdName}</td>              <td>${bean.name}</td>              <td><j:switch value="${bean.status}">                  <j:case value="1">未启用</j:case>                  <j:case value="2">已启用</j:case>                  <j:case value="3">已失效</j:case>                </j:switch></td>              <td>${bean.user.realName}</td>              <td>              <j:if test="${bean.invalid==false }">              <j:ifelse test="${bean.status eq 1}">                  <j:then>                    <a href="${ctx}/campus/flow/add/${bean.fdId}" class="btn btn-info">组织备课</a>                  </j:then>                  <j:else>                    <a href="${ctx}/campus/flow/edit/${bean.fdId}" class="btn btn-info">组织备课</a>                  </j:else>                </j:ifelse>                  </j:if>                 <!--                 <a href="${ctx}/campus/flow/deleteFlow/${bean.fdId}">删除</a>                 -->                </td>            </tr>          </j:iter>        </tbody>      </table>    </form>  </div></body></html>