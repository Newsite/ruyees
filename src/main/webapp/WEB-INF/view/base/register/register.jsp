<%@page pageEncoding="UTF-8"%><%@ taglib prefix="j" uri="/WEB-INF/tlds/formtag.tld"%><%@ taglib prefix="tags" tagdir="/WEB-INF/tags"%><j:set name="ctx" value="${pageContext.request.contextPath}" /><!DOCTYPE html><html lang="zh_CN"><head><link rel="stylesheet" href="${ctx}/resources/css/bootstrap.min.css"><link rel="stylesheet" href="${ctx}/resources/css/bootstrap-responsive.min.css"><link rel="stylesheet" href="${ctx}/resources/css/otp_iframe.css"><link rel="stylesheet" href="${ctx}/resources/uploadify/uploadify.css"></link><script type="text/javascript" src="${ctx }/resources/js/lib/jquery/jquery.min.js"></script><script type="text/javascript" src="${ctx }/resources/js/jquery.jalert.js"></script><script type="text/javascript" src="${ctx}/resources/js/lib/jquery-validation/1.10.0/jquery.validate.min.js"></script><script type="text/javascript" src="${ctx}/resources/js/validate2.js"></script><script type="text/javascript" src="${ctx}/resources/uploadify/jquery.uploadify-3.1.min.js?id=22"></script><style type="text/css">#adduser {  background-color: #FFF;  padding: 10px 20px;}#adduser .u_list>tbody>tr a {  cursor: pointer;  text-decoration: none}#inpUser {  width: 400px;}</style><script type="text/javascript">	jQuery(document).ready(function() {		jQuery("#inputForm").validate();		return;	});</script><script type="text/javascript">	$().ready(function() {jQuery.validator.addMethod("repassword", function(value, element) {		// 密码验证		var b = false;		if ($("#password").val() == value) {			b = true;		} else {			b = false;		}		return (this.optional(element) || /^[a-zA-Z]{1}([a-zA-Z0-9]|[._]){7,19}$/.test(value)) && b;},			"Please enter the same password.");		$("#repassword").blur(function() {			if ($("#password").val() != $("#repassword").val()) {				$("#password").val("");				$("#repassword").val("");			}		});		$("#fdIdentityCard").onchange(function() {			checkIdentityCard($("#fdIdentityCard").val());		});	});	function checkIdentityCard(str) {		if (str == null || str == "") {			if(!isIdCardNo(str)){				return false;			}			$("#fdIdentityCardLabel").hide();			return false;		}		$.ajax({			type : "post",			dataType : "json",			url : "${ctx}/ajax/register/checkIdentityCard",			data : {				str : str			},			success : function(data) {				if (data > 0) {					$("#fdIdentityCardLabel").html(							"this IdentityCard has registered.").show();					$("#fdIdentityCard").val("");				} else {					$("#fdIdentityCardLabel").hide();				}			}		});	}</script></head><body>  <form class="form-horizontal" method="post" id="inputForm" action="${ctx}/register2/save2" name="form">    <input name="soptId" type="hidden" class="span3" value="${sopt.fdId }" /> <input name="fdIsEmp" type="hidden"      class="span3" value="${sopt.fdIsEmp }" /> <input id="depatId" name="depatId" type="hidden" class="span3"      value="${sopt.depatId }" />    <section id="set-exam">      <div class="well">        <table class="table">          <tr>            <th>姓名</th>            <td colspan="2"><input name="notifyEntity.realName" type="text" class="span3 required userName"              value="${sopt.realName }" maxlength="4" readOnly /></td>          </tr>          <tr>            <th>身份证号(登录名)</th>            <td colspan="2"><input id="fdIdentityCard" name="fdIdentityCard" type="text"              class="span3 required isIdCardNo" value="" maxlength="18" /> <label id="fdIdentityCardLabel"              class="text-error" style="display: none;"></label></td>          </tr>          <tr>            <th>登录密码</th>            <td colspan="2"><input id="password" name="password" type="password" class="span3 required password"              value="" maxlength="15" /></td>          </tr>          <tr>            <th>确认登录密码</th>            <td colspan="2"><input id="repassword" name="password_2" type="password" class="span3 repassword"              value="" maxlength="15" /></td>          </tr>          <tr>            <th>性别</th>            <td colspan="2"><label class="radio inline"> 男<input type="radio" checked name="fdSex"                value="F" <j:if test="${bean.fdSex=='F'}">checked</j:if> />            </label> <label class="radio inline"> 女<input type="radio" name="fdSex" value="M"                <j:if test="${bean.fdSex=='M'}">checked</j:if> />            </label></td>          </tr>          <tr>            <th>部门</th>            <td colspan="2"><input id="deptName" name="deptName" type="text" class="span3 required"              value="${sopt.deptName }" readOnly /></td>          </tr>          <tr>            <th>电子邮箱</th>            <td colspan="2"><input name="notifyEntity.fdEmail" type="text" class="span3 email"              value="${sopt.fdEmail }" readOnly /></td>          </tr>          <!--          <tr>            <th>手机号码</th>            <td colspan="2"><input id="fdMobileNo" name="notifyEntity.fdMobileNo" type="text"              class="span3 isMobile" value="" maxlength="11" /></td>          </tr>          <tr>            <th>QQ</th>            <td colspan="2"><input name="fdQq" type="text" class="span3 qq" value="" maxlength="12" /></td>          </tr>          <tr>            <th>MSN</th>            <td colspan="2"><input name="fdMsn" type="text" class="span3 email" value="" /></td>          </tr>          -->          <tr>            <th>头像</th>            <td colspan="2"><img id="imgshow" src="${ctx }/" class="pull-right"> <tags:icoupload                fullname="fdIcoUrl" fullnameid="fdIcoUrl" folder="Folder" imgshow="imgshow" filename="" filevalue=""                fullvalue="${bean.backgroundUrl}" id="upIcon">              </tags:icoupload></td>          </tr>          <tr>            <td colspan="4"><button class="btn btn-info" type="submit" value="提交" id="saveBtn">确定</button></td>          </tr>        </table>      </div>    </section>  </form></body></html>