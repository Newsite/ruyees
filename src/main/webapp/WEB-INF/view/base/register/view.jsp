<%@page pageEncoding="UTF-8"%><%@ taglib prefix="j" uri="/WEB-INF/tlds/formtag.tld"%><%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%><j:set name="ctx" value="${pageContext.request.contextPath}" /><%@ taglib prefix="tags" tagdir="/WEB-INF/tags"%><!DOCTYPE html><html lang="zh_CN"><head></head><body>  <form class="form-horizontal" method="post" id="inputForm" action="${ctx}/register/view" name="form">    <input type="hidden" name="fdId" value="${bean.fdId}" />    <section id="set-exam">      <div class="well">        <div class="page-header">          <a href="${ctx}/register/list" class="tile-return">返回上页</a>          <h4>新教师注册</h4>          <p>在本模块中，您可以为新教师创建账号。</p>        </div>        <h5>基本信息</h5>        <table class="table">          <tr>            <th>头像</th>            <td colspan="3"><tags:image href="${bean.fdIcoUrl}" clas="img-polaroid img-face" /></td>          </tr>          <tr>            <th width="15%">姓名</th>            <td>${bean.realName}</td>            <th>账号</th>            <td>${bean.fdIdentityCard}</td>          </tr>          <tr>            <th width="15%">生日</th>            <td>${bean.fdBirthDay}</td>            <th>血型</th>            <td>${bean.fdBloodType}</td>          </tr>          <tr>            <th>电子邮箱</th>            <td>${bean.fdEmail}</td>            <th>微博ID</th>            <td>${bean.fdMicroBlog}</td>          </tr>          <tr>            <th>所获奖项/证书 </th>            <td colspan="3">${bean.fdCertificate}</td>          </tr>          <tr>            <th>爱好/特长</th>            <td colspan="3">我擅长/喜欢 <u>${bean.fdHobby}</u>, 我曾经<u>${bean.fdOldHobby}</u></td>          </tr>          <tr>            <th>最自豪之事</th>            <td colspan="3">${bean.fdPride}</td>          </tr>          <tr>            <th>最遗憾之事</th>            <td colspan="3">${bean.fdPity}</td>          </tr>          <tr>            <th>最想实现之事</th>            <td colspan="3">${bean.fdDram}</td>          </tr>        </table>        <h5>工作经历</h5>        <table class="table">	          <tr>	            <th>机构名称</th>	            <th>部门/职位</th>	            <th>工作时间</th>	            <th>工作成果</th>	          </tr>        	<j:iter items="${bean.workHistory}" var="work" status="vstatus">	          <tr>	          	<td>${work.orgName}</td>	            <td>${work.deptName}</td>	            <td>${work.beginYear}&nbsp;-&nbsp;${work.endYear}</td>	            <td>${work.product}</td>	          </tr>        	</j:iter>        </table>        <h5>教育背景</h5>        <table class="table">	          <tr>	            <th>学校名称</th>	            <th>入学时间</th>	            <th>专业名称</th>	          </tr>        	<j:iter items="${bean.education}" var="edu" status="vstatus">	          <tr>	          	<td>${edu.schoolName}</td>	            <td>${edu.beginYear}</td>	            <td>${edu.majorName}</td>	          </tr>        	</j:iter>        </table>      </div>    </section>  </form></body></html>